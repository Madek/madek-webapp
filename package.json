    "async": "1.5.0",
    "bootstrap": "3.3.5",
    "browserify": "11.2.0",
    "browserify-incremental": "3.0.1",
    "bulk-require": "0.2.1",
    "bulkify": "1.1.1",
    "check-dependencies": "0.11.0",
