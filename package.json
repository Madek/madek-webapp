{
  "name": "madek-webapp-ui",
  "description": "madek javascript toolchain",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "npm run -s lint && npm run -s test:jest",
    "test:jest": "jest",
    "lint": "bash -c 'npm run -s coffeelint -- --quiet app/assets/javascripts/{*.coffee,**/*.{coffee,cjsx}} && npm run -s standardlint -- {.,**}/*.js'",
    "devtools": "npm run -s build-devtools && coffee --nodejs \"-r ./tmp/devtools.js\"",
    "devtools-js": "npm run -s build-devtools && node -r ./tmp/devtools.js",
    "build-devtools": "npm run -s browserify -- app/assets/javascripts/developer-tools.coffee > tmp/devtools.js",
    "coffeelint": "coffeelint",
    "standardlint": "standard",
    "jest": "jest",
    "browserify": "browserify"
  },
  "dependencies": {
    "@eins78/typeahead.js": "0.11.2-pre.1",
    "active-lodash": "1.2.2",
    "ampersand-model": "6.0.2",
    "ampersand-react-mixin": "github:eins78/ampersand-react-mixin#1e7b0c4",
    "ampersand-rest-collection": "5.0.0",
    "any_sha1": "0.1.1",
    "async": "^1.5.2",
    "babyparse": "^0.4.3",
    "bootstrap": "^3.3.6",
    "browserify": "^11.2.0",
    "browserify-incremental": "3.0.1",
    "bulk-require": "0.2.1",
    "bulkify": "1.1.1",
    "check-dependencies": "0.11.0",
    "classnames": "^2.2.3",
    "coffee-reactify": "4.0.0",
    "coffee-script": "1.10.0",
    "global": "4.3.0",
    "hashblot": "0.4.5",
    "history": "^2.0.0",
    "hogan.js": "3.0.2",
    "jquery": "^2.2.1",
    "jquery-ujs": "^1.2.0",
    "local-links": "1.4.0",
    "moment": "^2.11.2",
    "pdfjs-dist": "^1.4.95",
    "po2json": "0.4.1",
    "qs": "^6.1.0",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-file-drop": "^0.1.8",
    "reactify": "1.1.1",
    "rusha": "0.8.3",
    "standard": "^6.0.7",
    "underscore.string": "^3.3.4",
    "uuid-validate": "0.0.2",
    "xhr": "^2.2.0",
    "yamlify": "0.1.2"
  },
  "devDependencies": {
    "babel-jest": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "big-list-of-naughty-strings": "1.0.0",
    "cheerio": "^0.20.0",
    "coffee-react": "^4.1.0",
    "coffee-react-transform": "3.3.0",
    "coffeelint": "^1.14.2",
    "jest-cli": "*",
    "react-addons-test-utils": "~0.14.0"
  },
  "browserify": {
    "transform": [
      "coffee-reactify",
      "reactify",
      "yamlify",
      "bulkify"
    ],
    "ignore": [
      "crypto"
    ]
  },
  "babel": {
    "presets": ["es2015", "react"]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jest-script-preprocessor",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/.",
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "testFileExtensions": [
      "coffee",
      "cjsx",
      "js",
      "jsx"
    ],
    "moduleFileExtensions": [
      "coffee",
      "cjsx",
      "js",
      "jsx"
    ]
  },
  "author": "Max Albrecht <max.albrecht@zhdk.ch>",
  "repository": "https://github.com/Madek/madek-webapp.git",
  "licenses": [
    {
      "type": "GNU GPLv3",
      "url": "http://www.gnu.org/copyleft/gpl.html"
    }
  ]
}
