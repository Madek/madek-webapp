{
  "name": "madek",
  "version": "1.0.0",
  "description": "madek javascript toolchain",
  "private": true,
  "dependencies": {
    "bootstrap": "^3.3.1",
    "jquery": "^2.1.3",
    "lodash": "^2.4.1",
    "moment": "^2.8.4",
    "po2json": "^0.3.2",
    "typeahead.js": "^0.10.5",
    "underscore.string": "^2.4.0"
  },
  "devDependencies": {
    "browserify": "^7.0.2",
    "browserify-shim": "^3.8.1",
    "coffeeify": "^1.0.0",
    "istanbul": "^0.3.5",
    "tape": "^3.0.3"
  },
  "transform": [ "browserify-shim", "coffeeify" ],
  "browserify-shim": {
    "bootstrap": {
      "exports": "bootstrap",
      "depends": {
        "jquery": "jQuery"
      }
    }
  },
  "scripts": {
    "test": "cd app/assets/javascripts/test && istanbul cover tape *.js",
    "build": "browserify app/assets/application.js.coffee"
  },
