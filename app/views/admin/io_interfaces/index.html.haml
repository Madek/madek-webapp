.page-header
  %h1
    %i.icon-media-entry
    IO Interfaces (#{@io_interfaces.total_count})

.row
  .col-xs-12
    .btn-group
      = link_to new_admin_io_interface_path, class: 'btn btn-default' do
        %i.glyphicon.glyphicon-plus
        Create IO Interface

.row
  .col-xs-12
    %table.table.table-striped
      %thead
        %tr
          %th ID
          %th Description
          %th
      %tbody
        = render @io_interfaces

= paginate @io_interfaces
