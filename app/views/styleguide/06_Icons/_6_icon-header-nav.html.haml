:ruby
  element = {
    name: 'Header Nav Icons',
    cssFile: '_icons.scss',
    cssSelector: '.icon-{name}',
    section: '6.6',
    mods: {
      nil => 'Default',
      'dark' => 'Midtone variation'
    }
  }

= render_partial('element', element) do
  - element[:mods].each do |name, description|
    = render_partial('example', { mod: name}) do

      = component('icon.nav-help', name)
      = component('icon.nav-search', name)
