@font-face {
  font-family: 'madek-icon-font';
  src:  url('<%= asset_path('madek-icon-font/fonts/madek-icon-font.eot') %>?b2rnhd');
  src:  url('<%= asset_path('madek-icon-font/fonts/madek-icon-font.eot') %>?b2rnhd#iefix') format('embedded-opentype'),
    url('<%= asset_path('madek-icon-font/fonts/madek-icon-font.ttf') %>?b2rnhd') format('truetype'),
    url('<%= asset_path('madek-icon-font/fonts/madek-icon-font.woff') %>?b2rnhd') format('woff'),
    url('<%= asset_path('madek-icon-font/fonts/madek-icon-font.svg') %>?b2rnhd#madek-icon-font') format('svg');
  font-weight: normal;
  font-style: normal;
  font-display: block;
}

[class^="icon-"], [class*=" icon-"] {
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'madek-icon-font' !important;
  speak: never;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

